{
  "$schema": "https://json.schemastore.org/chrome-manifest",
  "manifest_version": 3,
  "name": "Form Field Filler",
  "version": "1.2",
  "description": "Fills form fields with random values from a popup button.",
  "permissions": ["scripting", "activeTab", "sidePanel"],
  "host_permissions": ["<all_urls>"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Fill Form Fields"
  },
  "icons": {
    "16": "cat.png",
    "32": "cat.png",
    "48": "cat.png",
    "128": "cat.png",
    "256": "cat.png"
  }
}
